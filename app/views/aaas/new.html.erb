<h1>新しい投稿</h1>
<% if @aaa.errors.any? %>
  <%= @aaa.errors.count %>件のエラーが発生しました
  <ul>
    <% @aaa.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
  </ul>
<% end %>
<!--form_with部分-->
<%= form_with model:@aaa, url: '/aaas', method: :post do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>内容</h4>
  <%= f.text_area :body %>

  <h4>日付</h4>
  <%= f.text_field :date %>

  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <%= f.submit '投稿' %>
<% end %>